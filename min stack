class MinStack:

    def __init__(self):
        self.v=[]

    def push(self, val: int) -> None:
        return self.v.append(val)

    def pop(self) -> None:
        return self.v.pop()

    def top(self) -> int:
        return self.v[-1]

    def getMin(self) -> int:
        return min(self.v) 


# Your MinStack object will be instantiated and called as such:
# obj = MinStack()
# obj.push(val)
# obj.pop()
# param_3 = obj.top()
# param_4 = obj.getMin()
